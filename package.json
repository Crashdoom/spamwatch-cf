  "author": "https://github.com/Crashdoom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Crashdoom/spamwatch-js-fetch/issues"
  },
  "homepage": "https://github.com/Crashdoom/spamwatch-js-fetch",
  "dependencies": {
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.5.3",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
