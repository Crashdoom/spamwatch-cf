{
  "name": "spamwatch-js-fetch",
  "version": "0.2.1",
  "description": "A version of the Spamwat.ch API that uses the Fetch API",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --pretty",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
